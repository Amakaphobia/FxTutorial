<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.StackPane?>
<?import java.lang.String ?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<StackPane 
	xmlns:fx="http://javafx.com/fxml/1"
	styleClass="loginPane">
	
	<stylesheets>
		<String fx:value="app/background/root.css"/>
		<String fx:value="app/login/login.css"/>
	</stylesheets>
	
	<children>
		<fx:include
			fx:id="background"
			source="../background/BackGround.fxml"/>
		<Label
			text = "Welcome"
			styleClass="Welcome"
			StackPane.alignment="TOP_CENTER"/>
			
		<VBox
			styleClass="ButtonPanel">
			<children>
				<Label
					text="Please hit Start to Start"/>
				<Button 
					text="Start"
					onAction="#onStart"/>	
			</children>
		</VBox>
		
	</children>
</StackPane>

<!--fx:controller="login.LoginControl"-->